<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         version="3.1">

    <display-name>MIV Web</display-name>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>MIVController</servlet-name>
        <servlet-class>com.example.controller.MIVController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MIVController</servlet-name>
        <url-pattern>/MIV</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MonPengerjanPerProvUpiController</servlet-name>
        <servlet-class>com.example.controller.MonPengerjanPerProvUpiController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MonPengerjanPerProvUpiController</servlet-name>
        <url-pattern>/mon-pengerjaan-perprovupi</url-pattern>
    </servlet-mapping>

</web-app>
